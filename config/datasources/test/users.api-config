{
    "url": "https://api.example.com/users",
    "method": "GET",
    "headers": {
        "Authorization": "Bearer ${API_TOKEN}",
        "Content-Type": "application/json"
    },
    "query_params": {
        "limit": "100"
    },
    "fields": [
        {
            "name": "id",
            "type": "int",
            "nullable": false
        },
        {
            "name": "username",
            "type": "string",
            "nullable": false
        },
        {
            "name": "email",
            "type": "string",
            "nullable": true
        },
        {
            "name": "created_at",
            "type": "date",
            "nullable": false
        }
    ]
} 