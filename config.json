{
  "server": {
    "host": "0.0.0.0",
    "port": 3306,
    "server_version": "SqlExc"
  },
  "database": {
    "max_connections": 100,
    "idle_timeout": 3600
  },
  "log": {
    "level": "info",
    "format": "text"
  },
  "pool": {
    "goroutine_pool": {
      "max_workers": 10,
      "queue_size": 1000
    },
    "object_pool": {
      "max_size": 100,
      "min_idle": 2,
      "max_idle": 50
    }
  },
  "cache": {
    "query_cache": {
      "max_size": 1000
    },
    "result_cache": {
      "max_size": 1000
    },
    "schema_cache": {
      "max_size": 100
    }
  },
  "monitor": {
    "slow_query": {
      "max_entries": 1000
    }
  },
  "connection": {
    "max_open": 10,
    "max_idle": 5
  },
  "mvcc": {
    "enable_warning": true,
    "auto_downgrade": true,
    "xid_wrap_threshold": 100000,
    "max_active_txns": 10000
  },
  "session": {
  },
  "optimizer": {
    "enabled": true
  }
}

